<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="672.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.project1.Register">
   <children>
      <TextField fx:id="userNameField" layoutX="210.0" layoutY="40.0" onAction="#recieveUserName" onKeyTyped="#recieveUserName" prefHeight="38.0" prefWidth="177.0" promptText="user name*" />
      <PasswordField fx:id="passwordField" layoutX="212.0" layoutY="135.0" onAction="#recievePassword" onKeyTyped="#recievePassword" prefHeight="38.0" prefWidth="177.0" promptText="password*" />
      <TextField fx:id="emailFiled" layoutX="210.0" layoutY="214.0" onAction="#recieveEmail" onKeyTyped="#recieveEmail" prefHeight="38.0" prefWidth="177.0" promptText="email*" />
      <TextField fx:id="phoneNumberField" layoutX="210.0" layoutY="298.0" onAction="#recivePhoneNumber" onKeyTyped="#recivePhoneNumber" prefHeight="38.0" prefWidth="177.0" promptText="phonenumber" />
      <Button layoutX="253.0" layoutY="539.0" mnemonicParsing="false" onAction="#check" prefHeight="38.0" prefWidth="94.0" text="regisetr" />
      <Label fx:id="label" alignment="CENTER" layoutX="192.0" layoutY="478.0" prefHeight="31.0" prefWidth="212.0" />
      <ImageView fx:id="imageView" fitHeight="104.0" fitWidth="144.0" layoutX="226.0" layoutY="352.0" pickOnBounds="true" preserveRatio="true" />
      <Button layoutY="-4.0" mnemonicParsing="false" onAction="#back" prefHeight="31.0" prefWidth="70.0" text="back" />
      <Button fx:id="setProfile" layoutX="418.0" layoutY="392.0" mnemonicParsing="false" onAction="#setImage" text="Button" />
   </children>
</AnchorPane>
