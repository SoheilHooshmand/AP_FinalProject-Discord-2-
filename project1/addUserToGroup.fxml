<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.project1.AddUserToGroup">
   <children>
      <TextField fx:id="nameField" layoutX="126.0" layoutY="60.0" onAction="#enterName" onKeyTyped="#enterName" prefHeight="25.0" prefWidth="342.0" promptText="enter user name of person you want to add" />
      <Button fx:id="addUser" layoutX="249.0" layoutY="200.0" mnemonicParsing="false" onAction="#add" prefHeight="25.0" prefWidth="96.0" text="add" />
      <Button layoutY="2.0" mnemonicParsing="false" onAction="#back" prefHeight="25.0" prefWidth="65.0" text="back" />
      <Label fx:id="label" alignment="CENTER" layoutX="125.0" layoutY="260.0" prefHeight="25.0" prefWidth="342.0" />
   </children>
</AnchorPane>
